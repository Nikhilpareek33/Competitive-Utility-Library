//	#endif
  	
    fastio
	
	string line;
	stringstream s;
	
	s.clear();
	ll t=0;
	while(getline(cin,line))
	{
		
		if(t>0) cout<<endl;
		t++;
		init();
		s.clear();
		
		s << line;
		
		vector<char> a;
		char ch;
		
		while(s >> ch)
		{
			a.pb(ch);	
			present[ch-'a']=1;
			
		}
		
		s.clear();
		
		getline(cin,line);
		
		s << line;
		
		
		vll in(27,0),out(27,0);
		char c1,c2;
		while(s >> c1 >>c2)
		{
			ll f,s;
			
			f = c1-'a';
			
			s=c2-'a';
			
			g[f].pb(s);
			out[f]++;
			in[s]++;
//			cout<<c1 <<" > "<<c2<<endl;
		}
		
		vll vis(27,0);
		
		string ans;
		
//		fr(i,27)
//		{
//			sort(all(g[i]));
//		}
		solve(in,out,ans,vis);
		
//		cout<<endl;
	}
    
//    int t;
